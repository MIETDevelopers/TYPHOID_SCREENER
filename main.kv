<FirstPage>:
    name: "first_page"

    orientation: "vertical"

    #themechanger
    MDRoundFlatIconButton:
        text: 'Theme'
        icon: 'theme-light-dark'
        pos_hint:{'center_x':0.18,'center_y':0.9}
        font_size: '18sp'
        on_release: app.theme_changer()

    MDLabel:
        text: "Disclaimer"
        font_style: 'H5'
        pos_hint:{'center_x':0.8,'center_y':0.8}   
        
    MDLabel:
        text: "Welcome to Mboalab! \n This app is by no means a substitue of a established diagnostic procedures"
        halign: "center"    
        
    MDRectangleFlatButton:
        text: "Next"
        theme_text_color: "Custom"
        text_color: 0, 0, 1, 1
        line_color: 0, 0, 1, 1
        theme_icon_color: "Custom"
        icon_color: 0, 0, 1, 1
        valign: "center"
        pos_hint: {"center_x": 0.5, "center_y": 0.1}
        spacing: None
        on_press: root.manager.current = "second_page"                  
        #on_press: root.manager.current = 'eleventh_page'
    

