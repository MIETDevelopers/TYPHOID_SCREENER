<FourthPage>:    
    name: "fourth_page"    
        # create new patient records
        # Show patient symptoms form page #4    

        # below is kivy not kivy md except for the last buttons
    ScrollView:
        size: self.size
        MDGridLayout:
            size_hint_y: None
            height: self.minimum_height
            width: self.minimum_width
            cols: 1
            spacing: "20dp"
            padding: "20dp"
            MDTopAppBar:

                title: "Hi"
                id:toolbar
                elevation: 5
                pos_hint: {'top':0.5}
                md_bg_color: 200/255,10/255,30/255,1
            MDTextFieldRect:
                id: patient_name
                icon_left: "account-check"
                hint_text: "Patient Name"
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None  
                height: "30dp"
                width: 220
                font_size: 20
                pos_hint: {"center_x": 0.5}
            MDTextFieldRect:
                id: age
                icon_left: "map-marker"
                hint_text: "Age"
                input_filter: 'int'
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None  
                height: "30dp"
                width: 220
                font_size: 20            
                pos_hint: {"center_x": 0.5}                                   
            #city
            MDTextFieldRect:
                id: city
                icon_left: "city"    
                hint_text: "City"
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None 
                height: "30dp"
                width: 220
                font_size: 20
                pos_hint: {"center_x": 0.5}
            #state
            MDTextFieldRect:
                id: state
                icon_left: "city"
                hint_text: "State"
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None  
                height: "30dp"
                width: 220
                font_size: 20
                pos_hint: {"center_x": 0.5}
            #country
            MDTextFieldRect:
                id: country
                icon_left: "city"
                hint_text: "Country"
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None  
                height: "30dp"
                width: 220
                font_size: 20
                pos_hint: {"center_x": 0.5}
            #Gender
            MDTextFieldRect:
                id: gender
                icon_left: "gender"
                hint_text: "Gender"
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None  
                height: "30dp"
                width: 220
                font_size: 20
                pos_hint: {"center_x": 0.5}
            #phone number
            MDTextFieldRect:
                id: phone
                icon_left: "phone"
                hint_text: "Contact number"
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None  
                height: "30dp"
                width: 220
                font_size: 20
                pos_hint: {"center_x": 0.5}    
            #email    
            MDTextFieldRect:
                id: signup_email
                icon_left: "email"
                hint_text: "Email"
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None  
                height: "30dp"
                width: 220
                font_size: 20
                pos_hint: {"center_x": 0.5}
            #ethnicity
            #email    
            MDTextFieldRect:
                id: ethnicity
                icon_left: "email"
                hint_text: "Caucassian"
                foreground_color: (0,0,0,1)
                size_hint: 0.5,None  
                height: "30dp"
                width: 220
                font_size: 20
                pos_hint: {"center_x": 0.5}
            #DOB
            MDFloatLayout:
                MDRaisedButton:
                    id : date
                    text : "Record entry date"
                    pos_hint: {"center_x": 0.1,"center_y": 0.4} 
                    #on_release : app.show_date_picker(self, fourth_page)
                    on_release : app.show_date_picker( "fourth_page")  

            MDRectangleFlatButton:
                text: "SUBMIT"
                font_size: 15
                theme_text_color: "Custom"
                text_color: 0, 0, 1, 1
                pos_hint: {"center_x": 0.5}
                on_press: app.submit_pat_info()
            MDRectangleFlatButton:
                text: "EXISTING PATIENT"
                font_size: 15
                theme_text_color: "Custom"
                text_color: 0, 0, 1, 1
                pos_hint: {"center_x": 0.5}
                on_press: root.manager.current = "patient_features" # this needs to route to predict page
                 
            MDRectangleFlatButton:
                text: "BACK"
                font_size: 15
                pos_hint: {"center_x": 0.5}
                on_press: root.manager.current = "fifth_page" 
