<Doctechlogin>: 
    name: 'second_page'       
    orientation: 'vertical'
        
    MDTextFieldRound:
        id: user
        icon_left: "account-check"
        hint_text: "Name"
        foreground_color: (0,0,0,1)
        size_hint_x: None
        width: 220
        font_size: 20
        pos_hint: {"center_x": 0.5,"center_y": 0.6}
    MDTextFieldRound:
        id: password
        icon_left: "key-variant"
        hint_text: "Password"
        foreground_color: (0,0,0,1)
        size_hint_x: None
        width: 220
        font_size: 20            
        pos_hint: {"center_x": 0.5,"center_y": 0.5}
        password: True
    MDRectangleFlatButton:
        text: "LOG IN"
        font_size: 15
        theme_text_color: "Custom"
        text_color: 0, 0, 1, 1
        pos_hint: {"center_x": 0.5,"center_y": 0.3}
        on_press: root.Login()
    